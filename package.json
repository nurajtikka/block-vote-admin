{
    "name": "block-vote-admin",
    "version": "1.0.0",
    "private": true,
    "license": "UNLICENSED",
    "author": "Nuraj Tilakaratne",
    "scripts": {
        "dev": "yarn msg && PORT=30040 concurrently -k -n \"N,PROXY\" -i \"next dev\" \"yarn start-ssl-proxy\"",
        "build": "next build",
        "build-ssl-proxy": "yarn build && PORT=30040 concurrently -k -n \"N,PROXY\" \"next start\" \"yarn start-ssl-proxy\"",
        "msg": "echo \"\n\n--> https://localhost:3004/ \n\n\"",
        "start-ssl-dev-proxy": "local-ssl-proxy --source 3004 --target 3000",
        "start-ssl-proxy": "local-ssl-proxy --source 3004 --target $PORT",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@ant-design/icons": "^5.0.1",
        "@types/antd": "^1.0.0",
        "@types/eslint": "^8",
        "@types/node": "20.4.1",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "antd": "^5.4.5",
        "axios": "^1.4.0",
        "clsx": "1.2.1",
        "concurrently": "7.2.1",
        "eslint": "8.44.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-next": "13.4.9",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-json": "^2.1.2",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.21.3",
        "eslint-plugin-react-hooks": "^4.1.2",
        "eslint-plugin-sort-destructure-keys": "^1.3.5",
        "local-ssl-proxy": "^1.3.0",
        "moment": "^2.29.4",
        "next": "13.4.9",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^7.8.3",
        "postcss-rtl": "^2.0.0",
        "prettier": "^2.4.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sass": "1.56.1",
        "stylelint": "14.15.0",
        "stylelint-config-css-modules": "4.1.0",
        "stylelint-config-standard-scss": "6.1.0",
        "typescript": "5.1.6"
    },
    "engines": {
        "node": ">=16.0.0",
        "yarn": ">=1.22.0",
        "npm": "please-use-yarn"
    },
    "stylelint": {
        "extends": [
            "stylelint-config-standard-scss",
            "stylelint-config-css-modules"
        ],
        "plugins": [
            "stylelint-scss"
        ],
        "rules": {
            "indentation": 4,
            "selector-class-pattern": "^[a-z]([A-Z]?[a-z]+)+"
        }
    },
    "resolutions": {
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6"
    },
    "postcss": {
        "plugins": [
            "postcss-flexbugs-fixes",
            [
                "postcss-preset-env",
                {
                    "autoprefixer": {
                        "flexbox": "no-2009"
                    },
                    "stage": 3,
                    "features": {
                        "custom-properties": false
                    }
                }
            ],
            "postcss-rtl"
        ]
    },
    "packageManager": "yarn@4.0.0-rc.6"
}
